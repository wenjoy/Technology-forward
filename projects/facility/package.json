    "start": "node scripts/start.js",
    "start:dev": "webpack-dashboard -- node scripts/start.js",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "NODE_ENV=production serve -s build",
    "build": "node scripts/build.js",
    "build:local": "GENERATE_SOURCEMAP=true node scripts/build.js",
    "test": "node scripts/test.js -w 1",
    "release": "np --any-branch --no-cleanup --yolo --no-tests",
    "deploy": "./scripts/release.sh",
    "analyze:main": "source-map-explorer build/static/js/index.*",
    "analyze:bundle": "webpack-bundle-analyzer stats.json",
    "test:update": "npm test -- --updateSnapshot",
    "test:coverage": "npm test -- --coverage",
    "open:coverage": "open coverage/lcov-report/index.html",
    "lint": "eslint --fix './src/**/*.js' --quiet",
    "lint:style": "stylelint --fix './src/**/*.css'; exit 0",
    "lint:checkstyle": "eslint src -f checkstyle > facility-ui-eslint-checkstyle.xml",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "version": "conventional-changelog -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prebuild:i18n": "rimraf src/i18n",
    "build:i18n": "i18nassist compile"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.less": [
      "stylelint --fix",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  },
  "browserslist": [
    "> .5%",
    "last 2 versions",
    "not ie 11",
    "not op_mini all"
  ],
