{
  "name": "pos-native",
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios --simulator 'iPad Pro (9.7-inch) (13.5)'",
    "android": "rm -f android/app/build/outputs/apk/debug/active_pos_* && react-native run-android",
    "android-emulator": "emulator @Nexus_9_API_28",
    "android:connect": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "android:shake": "adb shell input keyevent 82",
    "android:log": "adb logcat *:E",
    "android:emulator-release": "ENVFILE=.env.qa react-native run-android --variant=release",
    "android:emulator": "ENVFILE=.env.qa react-native run-android",
    "multi": "concurrently -r 'yarn android' 'yarn ios'",
    "mock": "node ./mock-api/yoga",
    "lint": "eslint app/ index.js",
    "lint:fix": "eslint app/ index.js --fix",
    "test": "jest --watch",
    "test:clear": "jest --clearCache",
    "test:cov": "jest --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "coverage": "open ./coverage/lcov-report/index.html",
    "clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && yarn clear:apk:debug && yarn clear:apk:release && yarn clear:ipa",
    "clear:archive": "rm -rf ~/Library/Developer/Xcode/Archives && rm -rf ~/Library/Caches/com.apple.dt.Xcode",
    "clear:ipa": "rm -rf ios/build",
    "clear:apk:debug": "rm -f android/app/build/outputs/apk/debug/active_pos_*",
    "clear:apk:release": "rm -f android/app/build/outputs/apk/release/active_pos_*",
    "clear:all": "watchman watch-del-all && yarn clear && yarn clear:archive && rm -rf ./node_modules",
    "clear:xcode": "npx react-native-clean-project --remove-iOS-build --remove-iOS-pods",
    "build:android": "yarn clear:apk:release && cd android &&./gradlew assembleRelease && cd ../",
    "build:android:qa": "yarn clear:apk:release && cd android && ENVFILE=.env.qa ./gradlew assembleRelease && cd ../",
    "build:android:reg": "yarn clear:apk:release && cd android && ENVFILE=.env.regression ./gradlew assembleRelease && cd ../",
    "build:android:perf": "yarn clear:apk:release && cd android && ENVFILE=.env.perf ./gradlew assembleRelease && cd ../",
    "build:android:uat": "yarn clear:apk:release && cd android && ENVFILE=.env.uat ./gradlew assembleRelease && cd ../",
    "build:android:prod": "yarn clear:apk:release && cd android && ENVFILE=.env.prod ./gradlew assembleRelease && cd ../",
    "build:ios": "xcodebuild clean archive -workspace 'ios/PosNative.xcworkspace' -configuration 'Release' -archivePath 'ios/build/release/PosNative.xcarchive' -scheme 'PosNative'",
    "build:ios:qa": "xcodebuild clean archive -workspace 'ios/PosNative.xcworkspace' -configuration 'Release' -archivePath 'ios/build/release/PosNative.xcarchive' -scheme 'QA'",
    "build:ios:reg": "xcodebuild clean archive -workspace 'ios/PosNative.xcworkspace' -configuration 'Release' -archivePath 'ios/build/release/PosNative.xcarchive' -scheme 'Regression'",
    "build:ios:perf": "xcodebuild clean archive -workspace 'ios/PosNative.xcworkspace' -configuration 'Release' -archivePath 'ios/build/release/PosNative.xcarchive' -scheme 'Perf'",
    "build:ios:uat": "xcodebuild clean archive -workspace 'ios/PosNative.xcworkspace' -configuration 'Release' -archivePath 'ios/build/release/PosNative.xcarchive' -scheme 'Uat'",
    "build:ios:prod": "xcodebuild clean archive -workspace 'ios/PosNative.xcworkspace' -configuration 'Release' -archivePath 'ios/build/release/PosNative.xcarchive' -scheme 'Prod'",
    "build:export": "xcodebuild -exportArchive -archivePath ios/build/release/PosNative.xcarchive -exportPath ios/build/ipa/ -exportOptionsPlist DevExportOptions.plist",
    "build:export:prod": "xcodebuild -exportArchive -allowProvisioningUpdates -archivePath ios/build/release/PosNative.xcarchive -exportPath ios/build/ipa/ -exportOptionsPlist ProdExportOptions.plist",
    "build:simulator": "xcodebuild -workspace 'ios/PosNative.xcworkspace' -scheme 'PosNative' -configuration 'Release' -derivedDataPath 'ios/build/simulator' -destination 'generic/platform=iOS Simulator'",
    "build:simulator:qa": "xcodebuild -workspace 'ios/PosNative.xcworkspace' -scheme 'QA' -configuration 'Release' -derivedDataPath 'ios/build/simulator' -destination 'generic/platform=iOS Simulator'",
    "build:simulator:reg": "xcodebuild -workspace 'ios/PosNative.xcworkspace' -scheme 'Regression' -configuration 'Release' -derivedDataPath 'ios/build/simulator' -destination 'generic/platform=iOS Simulator'",
    "simulator:install": "xcrun simctl install booted ios/build/simulator/Build/Products/Release-iphonesimulator/PosNative.app",
    "increment:build": "react-native-version --increment-build",
    "version": "react-native-version --never-amend && git add .",
    "addtag": "yarn np --any-branch --no-cleanup --yolo"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "1.5.2",
    "@formatjs/intl-relativetimeformat": "4.5.9",
    "@react-native-community/checkbox": "0.5.2",
    "@react-native-community/netinfo": "4.7.0",
    "axios": "0.18.1",
    "axios-mock-adapter": "1.16.0",
    "bignumber.js": "8.1.1",
    "card-validator": "6.2.0",
    "intl": "1.2.5",
    "jetifier": "^1.6.5",
    "lodash.debounce": "4.0.8",
    "lodash.find": "4.6.0",
    "lodash.orderby": "4.6.0",
    "lodash.sortby": "4.7.0",
    "lodash.startswith": "4.2.1",
    "lodash.throttle": "4.1.1",
    "lodash.uniqby": "4.7.0",
    "lodash.uniqueid": "4.0.1",
    "lodash.uniqwith": "4.5.0",
    "moment": "2.24.0",
    "moment-range": "4.0.2",
    "react": "16.9.0",
    "react-intl": "3.11.0",
    "react-native": "0.61.5",
    "react-native-awesome-card-io": "0.9.0-rc.1",
    "react-native-barcode-mask": "1.1.0",
    "react-native-calendars": "1.259.0",
    "react-native-camera": "3.22.0",
    "react-native-config": "0.11.7",
    "react-native-elements": "1.0.0",
    "react-native-fast-img": "^0.0.1",
    "react-native-flash": "0.2.2",
    "react-native-gesture-handler": "1.5.5",
    "react-native-keyboard-manager": "4.0.13-16",
    "react-native-linear-gradient": "2.5.6",
    "react-native-localize": "1.3.3",
    "react-native-offline": "5.1.0",
    "react-native-permissions": "2.0.10",
    "react-native-root-toast": "3.2.0",
    "react-native-signature-capture": "0.4.10",
    "react-native-splash-screen": "3.2.0",
    "react-native-system-setting": "1.7.4",
    "react-native-uuid-generator": "6.1.1",
    "react-native-vector-icons": "6.6.0",
    "react-native-webview": "8.0.6",
    "react-navigation": "3.6.1",
    "react-navigation-backhandler": "1.3.0",
    "react-navigation-fluid-transitions": "0.3.1",
    "react-redux": "7.1.3",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "5.10.0",
    "redux-persist-filesystem-storage": "2.1.0",
    "redux-saga": "1.0.5",
    "rn-fetch-blob": "0.11.2",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@redux-saga/testing-utils": "1.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.9.0",
    "concurrently": "4.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-adapter-react-helper": "1.3.7",
    "enzyme-adapter-utils": "1.13.0",
    "enzyme-react-16-adapter-setup": "0.1.0",
    "enzyme-shallow-equal": "1.0.1",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-html": "^6.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "eslint-plugin-react-native": "3.6.0",
    "eslint-plugin-sonarjs": "0.3.0",
    "husky": "1.3.1",
    "jest": "24.9.0",
    "jest-enzyme": "7.1.2",
    "lint-staged": "8.1.0",
    "metro-react-native-babel-preset": "0.58.0",
    "np": "5.2.1",
    "react-dom": "16.12.0",
    "react-native-version": "3.2.0",
    "react-test-renderer": "16.12.0",
    "redux-mock-store": "1.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "{app/**/*,index}.js": [
        "yarn lint:fix",
        "git add"
      ]
    }
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/config/**",
      "!app/utils/ams*",
      "!app/services/EventSource/EventSource.js",
      "!app/index.ios.js",
      "!app/index.android.js",
      "!app/components/Scanner/index.js",
      "!app/saga/cardReaderAndroid.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js",
      "./setup-tests.js"
    ],
    "setupFiles": [
      "enzyme-react-16-adapter-setup",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|)"
    ],
    "testEnvironment": "node"
  }
}
